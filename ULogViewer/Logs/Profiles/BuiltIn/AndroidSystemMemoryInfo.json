{
  "DataSource": {
    "Name": "StandardOutput",
    "Options": {
      "Command": "adb shell dumpsys meminfo"
    }
  },
  "Icon": "Android",
  "IsContinuousReading": true,
  "LogChartSeriesSources": [
    {
      "PropertyDisplayName": "Using",
      "PropertyName": "Extra3"
    },
    {
      "PropertyDisplayName": "Using",
      "PropertyName": "Extra4",
      "SecondaryPropertyDisplayName": "PSS"
    },
    {
      "PropertyDisplayName": "Using",
      "PropertyName": "Extra5",
      "SecondaryPropertyDisplayName": "Kernel"
    },
    {
      "PropertyDisplayName": "PSS",
      "PropertyName": "Extra10",
      "SecondaryPropertyDisplayName": "Dalvik"
    },
    {
      "PropertyDisplayName": "PSS",
      "PropertyName": "Extra11",
      "SecondaryPropertyDisplayName": "Native"
    }
  ],
  "LogChartType": "ValueLinesWithDataPoints",
  "LogChartValueGranularity": "Kilobytes",
  "LogPatternMatchingMode": "ArbitraryAfterFirstMatch",
  "LogPatterns": [
    {
      "Regex": "^\\s*Total PSS by category",
      "IgnoreCase": true
    },
    {
      "Regex": "^\\s*(?<Extra11>[\\d\\,]+)\\s*KB?: Native",
      "IgnoreCase": true
    },
    {
      "Regex": "^\\s*(?<Extra10>[\\d\\,]+)\\s*KB?: Dalvik\\s*$",
      "IgnoreCase": true
    },
    {
      "Regex": "^\\s*Total RAM:\\s*(?<Extra1>[\\d\\,]+)\\s*K",
      "IgnoreCase": true
    },
    {
      "Regex": "^\\s*Free RAM:\\s*(?<Extra2>[\\d\\,]+)\\s*K",
      "IgnoreCase": true
    },
    {
      "Regex": "^\\s*Used RAM:\\s*(?<Extra3>[\\d\\,]+)\\s*KB?\\s*\\((?<Extra4>[\\d\\,]+)\\s*KB? used pss \\+ (?<Extra5>[\\d\\,]+)\\s*KB? kernel",
      "IgnoreCase": true
    },
    {
      "Regex": "^\\s*(ION|DMA-BUF):\\s*(?<Extra6>[\\d\\,]+)\\s*K",
      "IgnoreCase": true,
      "IsSkippable": true
    },
    {
      "Regex": "^\\s*GPU:\\s*(?<Extra7>[\\d\\,]+)\\s*K",
      "IgnoreCase": true,
      "IsSkippable": true
    }
  ],
  "RestartReadingDelay": 1000,
  "SortKey": "ReadTime",
  "TimestampCategoryGranularity": "Minute",
  "VisibleLogProperties": [
    {
      "DisplayName": "Timestamp",
      "Name": "ReadTime",
      "Width": 200
    },
    {
      "DisplayName": "Available",
      "Name": "Extra2",
      "Quantifier": "KB",
      "Width": 100
    },
    {
      "DisplayName": "Using",
      "Name": "Extra3",
      "Quantifier": "KB",
      "Width": 100
    },
    {
      "DisplayName": "Using",
      "Name": "Extra5",
      "Quantifier": "KB",
      "SecondaryDisplayName": "Kernel",
      "Width": 100
    },
    {
      "DisplayName": "Using",
      "Name": "Extra4",
      "Quantifier": "KB",
      "SecondaryDisplayName": "PSS",
      "Width": 100
    },
    {
      "DisplayName": "PSS",
      "Name": "Extra10",
      "Quantifier": "KB",
      "SecondaryDisplayName": "Dalvik",
      "Width": 100
    },
    {
      "DisplayName": "PSS",
      "Name": "Extra11",
      "Quantifier": "KB",
      "SecondaryDisplayName": "Native"
    }
  ]
}